// app global css
@import '~variables'

.q-table td
  white-space normal

body
  color: $dark

h1, h2, h3, h4, h5, h6
  margin-top: 0
//   color: $tertiary

.q-layout-drawer
  & .q-item
    padding: 12px 16px
  & .q-item-separator-component
    margin: 0
  & .q-item-section + .q-item-section
    margin-left: 0px
  & .q-collapsible-sub-item 
    & .q-item
      padding: 8px 16px 8px 0px
      & .q-item-side > .q-item-icon
        font-size: 18px
      & .q-item-main > .q-item-label
        font-size: 0.9rem  
